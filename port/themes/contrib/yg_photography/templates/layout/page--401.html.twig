            {% if twitter %}
                  <li><a href="{{twitter}}"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
            {% endif %}
            {% if facebook %}
                  <li><a href="{{facebook}}"><i class="fa fa-facebook-f" aria-hidden="true"></i></a></li>
            {% endif %}
            {% if pinterest %}
                    <li><a href="{{pinterest}}"><i class="fa fa-pinterest" aria-hidden="true"></i></a></li>
            {% endif %}
            {% if vimeo %}
                  <li><a href="{{vimeo}}"><i class="fa fa-vimeo" aria-hidden="true"></i></a></li>
            {% endif %}
            {% if instagram %}
                  <li><a href="{{instagram}}"><i class="fa fa-instagram" aria-hidden="true"></i></a></li>
            {% endif %}
        </ul>
      </div> 
    </div>
  </footer>
<!-- End footer Section -->  