<!-- End NavBar -->

<div class="main-menu-area fixed-top" id="main-menu">
  <div class="container">
      <nav class="navbar navbar-expand-lg navbar-white site-navigation navbar-togglable">
          {{page.logo}}
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
              <span class="fa fa-bars"></span>
          </button>
          {{page.primary_menu}}
      </nav>
  </div>
</div>
<!-- End Navbar -->

{% set url = url('<current>') %}
{% if 'user' in url|render|render %}
<!-- Top Banner Section -->

<div class="top-banner" style="background-image:url('{{banner_image}}');">
    <div class="container top-banner-caption text-center">
      <h1>{{page['#title']}}</h1>
    </div>
</div>

<!-- End Top Banner Section -->
{% endif %}

{# Main #}
<section id="not-found-section" class="text-center">
  <div class="container">               
    <div class="row"> 
      <div class="not-found-body" >
        <div class="not-title" >
          <h1>401</h1>
          <h2>Unauthorized Response Error</h2> 
        </div>  
          <h2>Access To The Webpage Was Denied. </h2>
          <p class="not-p">You are not authorized to access this page. You must login to view this page.</p>
          <a href="{{front_page}}"> Go Home </a>
      </div>
    </div>          
  </div>    
</section>
{# Main #}

{% if page.instagram_feeds %}
  <section id="instagram-feed" class="wow fadeInUp text-center"> 
        <div class="container">
          {{page.instagram_feeds}}
        </div>
  </section>
{% endif %}

<!-- Start footer section -->
