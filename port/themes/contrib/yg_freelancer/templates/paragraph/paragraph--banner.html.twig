<header id="fh5co-header" class="fh5co-cover js-fullheight" role="banner" style="background-image:url({{content.field_background_image.0}});" data-stellar-background-ratio="0.5">
		<div class="overlay"></div>
		<div class="container">
			<div class="row">
				<div class="col-md-8 col-md-offset-2 text-center">
					<div class="display-t js-fullheight">
						<div class="display-tc js-fullheight animate-box" data-animate-effect="fadeIn">
							<div class="profile-thumb" style="background: url({{content.field_image.0}});"></div>
							<h1><span>{{content.field_member_name.0}}</span></h1>
							<h3><span>{{content.field_member_designation.0}}</span></h3>
							<p>
								<ul class="fh5co-social-icons">
									
									{% if content.field_twitter_link.0 %}
										<li><a href="{{content.field_twitter_link.0['#url']}}">
										<i class="icon-twitter2"></i></a></li>
									{% endif %}
									{% if content.field_facebook_link.0 %}
										<li><a href="{{content.field_facebook_link.0['#url']}}">
										<i class="icon-facebook2"></i></a></li>
									{% endif %}
									{% if content.field_linkedin_link.0 %}
										<li><a href="{{content.field_linkedin_link.0['#url']}}">
										<i class="icon-linkedin2"></i></a></li>
									{% endif %}
									{% if content.field_dribbble_link.0 %}
										<li><a href="{{content.field_dribbble_link.0['#url']}}">
										<i class="icon-dribbble2"></i></a></li>
									{% endif %}
										
								</ul>
							</p>
						</div>
					</div>
				</div>
			</div>
		</div>
	</header>